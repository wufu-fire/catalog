{
    "name": "catlog",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
        "declarativeContent",
        "activeTab",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "*://git.dianpingoa.com/*/browse*",
                "*://git.dianpingoa.com/*/browse"
            ],
            "js": [
                "insert.js",
                "./static/index.js"
            ],
            "css": [
                "./static/index.css"
            ]
        }
    ],
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
}